{
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": true,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ],
  "presets": [
    [
      "env",
      {
        "modules": false,
        "debug": false,
        "targets": {
          "browsers": [
            "> 5%",
            "last 2 versions",
            "ie > 8"
          ]
        }
      }
    ]
  ]
}
